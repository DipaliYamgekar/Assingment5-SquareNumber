    "class point:\n",
    "    def __init__(self, x , y , z):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "        self.z=z\n",
    "    def sqSum(self):\n",
    "        return self.x**2+self.y**2+self.z**2\n",
    "p=point(1,3,5)\n",
    "print(p.sqSum())\n",
    "        \n",
